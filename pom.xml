<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>SpringCertificationProject</groupId>
  <artifactId>SpringCertificationProject</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  <build>
		<sourceDirectory>src</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<failOnMissingWebXml>true</failOnMissingWebXml>
				</configuration>
			</plugin>
			
			
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-clean-plugin</artifactId>
				<version>2.4.1</version>
				<configuration>
					<filesets>
						<fileset>
							<directory>C:/Tomcat_7/webapps/SpringCertificationProject</directory>
							<includes>
								<include>**/*</include>
							</includes>
							<followSymlinks>false</followSymlinks>
						</fileset>
					</filesets>
				</configuration>
			</plugin>
			
						<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.4</version>
				<executions>

					<execution>
						<id>copy-war</id>
						<phase>install</phase>
						<goals>
							<goal>copy-resources</goal>
						</goals>

						<configuration>
							<outputDirectory>C:/Tomcat_7/webapps/SpringCertificationProject</outputDirectory>
							<resources>
								<resource>
									<directory>target/SpringCertificationProject</directory>

								</resource>
							</resources>
						</configuration>
					</execution>
					
					<execution>
							<id>copy-war2</id>
							<phase>install</phase>
							<goals>
								<goal>copy-resources</goal>
							</goals>


							<configuration>
								<outputDirectory>C:/Tomcat_7/webapps/SpringCertificationProject/WEB-INF/classes</outputDirectory>
								<resources>
									<resource>
										<directory>resources</directory>
	
									</resource>
								</resources>
							</configuration>

						</execution>



				</executions>
			</plugin>
			
			
		</plugins>
			<finalName>SpringCertificationProject</finalName>
  </build>
  
